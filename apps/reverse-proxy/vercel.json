{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "https://e2b-landing-page.framer.website"
    },
    {
      "source": "/docs(/.*)?",
      "destination": "https://e2b-docs.vercel.app/docs$1"
    },
    {
      "source": "/dashboard(/.*)?",
      "destination": "https://e2b-dashboard.vercel.app/dashboard$1"
    }
  ]
}