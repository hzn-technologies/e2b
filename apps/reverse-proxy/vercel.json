{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "https://e2b-landing-page.framer.website",
      "has": [
        {
          "type": "host",
          "value": "e2b.dev"
        }
      ]
    },
    {
      "source": "/docs(/.*)?",
      "destination": "https://e2b-docs.vercel.app/docs$1",
      "has": [
        {
          "type": "host",
          "value": "e2b.dev"
        }
      ]
    },
    {
      "source": "/dashboard(/.*)?",
      "destination": "https://e2b-dashboard.vercel.app/dashboard$1",
      "has": [
        {
          "type": "host",
          "value": "e2b.dev"
        }
      ]
    },
    {
      "source": "/changelog(/.*)?",
      "destination": "https://e2b-changelog.framer.website$1",
      "has": [
        {
          "type": "host",
          "value": "e2b.dev"
        }
      ]
    },
    {
      "source": "/blog(/.*)?",
      "destination": "https://e2b-blog.framer.website$1",
      "has": [
        {
          "type": "host",
          "value": "e2b.dev"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)?",
      "has": [
        {
          "type": "host",
          "value": "blog.e2b.dev"
        }
      ],
      "destination": "https://e2b.dev/blog/$1",
      "permanent": true
    },
    {
      "source": "/(.*)?",
      "has": [
        {
          "type": "host",
          "value": "changelog.e2b.dev"
        }
      ],
      "destination": "https://e2b.dev/changelog/$1",
      "permanent": true
    },
    {
      "source": "/(.*)?",
      "has": [
        {
          "type": "host",
          "value": "docs.e2b.dev"
        }
      ],
      "destination": "https://e2b.dev/docs/$1",
      "permanent": true
    }
  ]
}
