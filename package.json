{
  "name": "ai-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx concurrently \"next dev\" \"cd api-service && poetry run flask --app app.py --debug run\"",
    "dev:api": "cd api-service && poetry run flask --app app.py --debug run",
    "build": "next build",
    "install:api": "cd api-service && poetry install",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "db:generate": "prisma generate",
    "db:pull": "prisma db pull",
    "supabase:generate": "supabase gen types typescript --project-id \"ntjfcwpzsxugrykskdgi\" --schema public > db/supabase.ts",
    "postinstall": "yarn db:generate"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@supabase/auth-helpers-nextjs": "^0.5.5",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.10.0",
    "@types/randomcolor": "^0.5.7",
    "clsx": "^1.2.1",
    "express": "^4.18.2",
    "human-id": "^4.0.0",
    "immer": "^9.0.19",
    "lucide-react": "^0.122.0",
    "nanoid": "^4.0.1",
    "next": "13.2.3",
    "next-superjson-plugin": "^0.5.6",
    "randomcolor": "^0.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-keys": "^2.7.2",
    "react-textarea-autosize": "^8.4.0",
    "sonner": "^0.1.6",
    "superjson": "^1.12.2",
    "swr": "^2.0.4",
    "usehooks-ts": "^2.9.1",
    "zustand": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.15",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prisma": "^4.11.0",
    "supabase": "^1.41.6",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vercel": "^28.16.12"
  }
}